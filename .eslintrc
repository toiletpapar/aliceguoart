{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-react"],
  "rules": {
    "no-debugger": "off",
    "brace-style": ["error", "1tbs"],
    "comma-dangle": [1, "always-multiline"],
    "no-multiple-empty-lines": 0,
    "no-useless-escape": 0,
    "react/prop-types": 0,
    "spaced-comment": 0
  },
  "globals": {
    "__DEV__": true
  }
}
